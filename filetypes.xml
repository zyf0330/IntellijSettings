<application>
  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*.pyc;*.pyo;*.rbc;*~;.DS_Store;.git;.hg;.svn;CVS;RCS;SCCS;__pycache__;_svn;rcs;" />
    <extensionMap>
      <removed_mapping ext="css" type="CSS" />
      <mapping ext="applejs" type="JavaScript" />
      <mapping ext="gradle.kts" type="Kotlin" />
      <mapping ext="main.kts" type="Kotlin" />
      <mapping pattern=".gitkeep" type="PLAIN_TEXT" />
      <mapping ext="tmp" type="PLAIN_TEXT" />
      <mapping ext="mo" type="PLAIN_TEXT" />
      <removed_mapping ext="apk" approved="true" type="ARCHIVE" />
      <removed_mapping ext="csv" approved="true" type="CSV/TSV" />
      <removed_mapping ext="log" approved="true" type="ANSI Aware" />
      <removed_mapping ext="psd" approved="true" type="Image" />
      <removed_mapping ext="tsv" approved="true" type="CSV/TSV" />
    </extensionMap>
  </component>
</application>